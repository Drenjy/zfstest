clobber: clean
	@$(RMFILES)	$(_STF_EXECUTABLES) \
			$(_STF_LIBRARIES) \
			${_STF_CONFIGURE} \
			${_STF_UNCONFIGURE} \
			${_STF_SETUP} \
			${_STF_CLEANUP} \
			${_STF_TESTCASES} \
			${_STF_TESTCASES_GEN} \
			${STF_CLOBBERFILES} \
			stf_description
